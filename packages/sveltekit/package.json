{
	"name": "@mecha_agent_inference_client/sveltekit",
	"description": "The Mecha Agent inference client implemented in Svelte for Sveltekit apps",
	"version": "1.0.0",
	"private": false,
	"type": "module",
	"scripts": {
		"build:dev": "vite build && npm run prepack",
		"build:prod": "vite build && npm run prepack",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"prepack": "svelte-kit sync && svelte-package && publint",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"files": [
		"dist"
	],
	"sideEffects": [
		"**/*.css"
	],
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./server": "./dist/server.js"
	},
	"peerDependencies": {
		"svelte": "^5.0.0"
	},
	"dependencies": {
		"@mecha_agent_inference_client/core": "workspace:*"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"publint": "^0.3.2",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"typescript": "^5.0.0",
		"vite": "^6.2.6"
	},
	"author": "Abdulrhman Goni",
	"license": "ISC",
	"keywords": [
		"Mecha",
		"Mecha Agent",
		"inference",
		"client",
		"AI",
		"Agent",
		"svelte",
		"sveltekit"
	]
}